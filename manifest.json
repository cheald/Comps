{
  "name": "Comps",
  "version": "0.1.2",
  "description": "Overlay comps onto your webpage for comparison",
  "browser_action": {
    "default_icon": "images/icon.png"
  },
  "background_page": "background.html",
  "permissions": [ "tabs", "http://*/", "https://*/" ],
  "icons": {
    "128": "images/128.png",
    "64": "images/64.png",
    "32": "images/32.png",
    "16": "images/16.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["css/content.css"],
      "js": ["js/content.js"]
    }
  ]
}
