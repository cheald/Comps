{
  "name": "Comps",
  "version": "0.1.1",
  "description": "Overlay comps onto your webpage for comparison",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background_page": "background.html",
  "permissions": [ "tabs", "http://*/" ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["content.css"],
      "js": ["content.js"]
    }
  ]
}
